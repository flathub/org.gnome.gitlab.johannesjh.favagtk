{
  "app-id": "org.gnome.gitlab.johannesjh.favagtk",
  "runtime": "org.gnome.Platform",
  "runtime-version": "43",
  "sdk": "org.gnome.Sdk",
  "command": "favagtk",
  "finish-args": [
    "--share=network",
    "--share=ipc",
    "--socket=fallback-x11",
    "--device=dri",
    "--socket=wayland",
    "--filesystem=host"
  ],
  "cleanup": [
    "/include",
    "/lib/pkgconfig",
    "/man",
    "/share/doc",
    "/share/gtk-doc",
    "/share/man",
    "/share/pkgconfig",
    "*.la",
    "*.a"
  ],
  "modules": [
    "requirements/blueprint-compiler.json",
    "requirements/python3-main.json",
    {
      "name": "favagtk",
      "builddir": true,
      "buildsystem": "meson",
      "sources": [
        {
          "type": "git",
          "url": "https://gitlab.gnome.org/johannesjh/favagtk.git",
          "tag": "v0.2.4",
          "commit": "c611fbbb23f1f40df0ca1cf06c859c56d2bb7d32"
        }
      ],
      "config-opts": ["-Dprofile=release"]
    }
  ]
}
